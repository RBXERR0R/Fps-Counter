local FSM = Instance.new("ScreenGui")
local FSH = Instance.new("TextLabel")
local FSC = Instance.new("UICorner")

FSM.Name = "FSM"
FSM.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
FSM.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
FSM.ResetOnSpawn = false

FSH.Name = "max"
FSH.Parent = LH
FSC.Parent = FSH
FSH.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
FSH.BorderColor3 = Color3.fromRGB(0, 0, 0)
FSH.Position = UDim2.new(0.2, 0, 0.1, 0)
FSH.Size = UDim2.new(0, 130, 0, 33)
FSH.Font = Enum.Font.SourceSans
FSH.Text = "Label"
FSH.TextColor3 = Color3.fromRGB(255, 255, 155)
FSH.TextSize = 18.000
FSH.BackgroundTransparency = 0.4
FSH.Active = true
FSH.Draggable = true

local texLabel = M4R

local runService = game:GetService("RunService")

local lastFrame = tick() -- movido para fora da função

runService.RenderStepped:Connect(function()
    local currentTime = tick()
    
    local deltaTime = currentTime - lastFrame -- cálculo do deltaTime antes de redefinir lastFrame
    
    lastFrame = currentTime
    
    local currentFPS = 1 / deltaTime
    texLabel.Text = "FPS: " .. math.floor(currentFPS)
end)
